#include<stdio.h>
void main()
{
int a[5],key,i,flag=0,n;
printf("enter the size of the array\n");
scanf('%d",&n);  
printf("enter the elements of the array\n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("elements of the array a are\n");
for(i=0;i<5;i++)
printf("%d",a[i]);
printf("enter the element to be searched\n");
scanf("%d",&key);
for(i=0;i<n;i++)
{
if(key==a[i])
{
flag=1;
break;
}
}
if(flag==1)
printf("element found\n");
else
printf("element not found\n");
}
